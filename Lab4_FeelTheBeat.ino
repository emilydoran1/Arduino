#include <Servo.h>Servo myServo;const int switchPin = 8;unsigned long previousTime = 0;int switchState = 0;int prevSwitchState = 0;int led = 4;long interval = 2000;const int a = 440;const int b = 466;const int c = 261;const int d = 294; const int e = 329; const int f = 349; const int g = 391;void setup() {  myServo.attach(10);  Serial.begin(9600);  // pinMode(ledPin, OUTPUT);  // digitalWrite(ledPin, HIGH);    for(int x = 4; x < 8; x++){    pinMode(x, OUTPUT);  }  pinMode(switchPin, INPUT);}void loop() {  unsigned long currentTime = millis();  if(currentTime - previousTime > interval){    previousTime = currentTime;    digitalWrite(led, HIGH);    led++;        if(led == 8){      playSong();      // blinkLight(6);      // blinkLight(4);      moveServo();    }  }  switchState = digitalRead(switchPin);    //reset the lights  if((switchState != prevSwitchState) && (led != 8)){        for(int x = 4; x < 8; x++){      digitalWrite(x, LOW);    }    led = 4;    previousTime = currentTime;  }  prevSwitchState = switchState;}//function to start the songvoid playSong(){  blinkLight(4);  tone(9, c, 200);  delay(100);    blinkLight(4);  tone(9, c, 200);  delay(100);    blinkLight(4);  tone(9, c, 200);  delay(75);    blinkLight(5);  tone(9, g, 200);  delay(100);    blinkLight(7);  tone(9, a, 200);  delay(100);  blinkLight(7);  tone(9, a, 200);  delay(100);    blinkLight(5);  tone(9, g, 200);  delay(100);    delay(300);      blinkLight(7);  tone(9, e, 200);  delay(120);    blinkLight(7);  tone(9, e, 200);  delay(120);    blinkLight(6);  tone(9, d, 200);  delay(120);    blinkLight(6);  tone(9, d, 200);  delay(120);    blinkLight(4);  tone(9, c, 200);  delay(120);    delay(500);    blinkLight(5);  tone(9, g, 200);  delay(120);    blinkLight(4);  tone(9, c, 200);  delay(120);    blinkLight(4);  tone(9, c, 200);  delay(120);    blinkLight(4);  tone(9, c, 200);